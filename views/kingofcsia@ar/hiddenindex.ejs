<% include ../parts/head %>
<% include ../parts/othernav %>

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">관리자 - AR 신청현황</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">

          <div class="panel-body">

            <table id="hiddentable" class="table table-striped table-bordered" cellspacing="0" width="100%">
             <thead>
              <tr>
                <th class="current">신청시간</th>
                <th class="arorop">분류</th>
                <th class="room">세미나실</th>
                <th class="purpose">이용목적</th>
                <th class="name">신청자</th>
                <th class="sort">계열</th>
                <th class="sex">성별</th>
                <th class="currentgrade">학년</th>
              </tr>
             </thead>

             <tbody>
              <% if(arenroll == null || arenroll.length == 0){ %>
               <tr>
                <td class="noData" colspan=100> 앗, 아무도 신청하지 않았네요! </td>
               </tr>
              <% } %>

              <% arenroll.forEach(function(arenroll) { %>
               <tr>
                <td class="current">
                   <div><%= arenroll.artwoonecurrent %></div>
                </td>
                <td class="arorop">
                  <div><%= arenroll.twoone %></div>
                </td>
                <td class="room">
                 <div><%= arenroll.artwooneroom %></div>
                </td>
                <td class="purpose">
                 <div><%= arenroll.artwoonepurpose %></div>
                </td>
                <td class="name">
                  <div><%= arenroll.artwoonename %></div>
                </td>
                <td class="sort">
                 <div><%= arenroll.artwoonesort %></div>
                </td>
                <td class="sex">
                 <div><%= arenroll.artwoonesex %></div>
                </td>
                <td class="currentgrade">
                 <div><%= arenroll.artwoonecurrentgrade %></div>
                </td>
              </tr>

              <tr>
               <td class="current">
                  <div><%= arenroll.artwotwocurrent %></div>
               </td>
               <td class="arorop">
                 <div><%= arenroll.twotwo %></div>
               </td>
               <td class="room">
                <div><%= arenroll.artwotworoom %></div>
               </td>
               <td class="purpose">
                <div><%= arenroll.artwotwopurpose %></div>
               </td>
               <td class="name">
                 <div><%= arenroll.artwotwoname %></div>
               </td>
               <td class="sort">
                <div><%= arenroll.artwotwosort %></div>
               </td>
               <td class="sex">
                <div><%= arenroll.artwotwosex %></div>
               </td>
               <td class="currentgrade">
                <div><%= arenroll.artwotwocurrentgrade %></div>
               </td>
             </tr>

             <tr>
              <td class="current">
                 <div><%= arenroll.artwothreecurrent %></div>
              </td>
              <td class="arorop">
                <div><%= arenroll.twothree %></div>
              </td>
              <td class="room">
               <div><%= arenroll.artwothreeroom %></div>
              </td>
              <td class="purpose">
               <div><%= arenroll.artwothreepurpose %></div>
              </td>
              <td class="name">
                <div><%= arenroll.artwothreename %></div>
              </td>
              <td class="sort">
               <div><%= arenroll.artwothreesort %></div>
              </td>
              <td class="sex">
               <div><%= arenroll.artwothreesex %></div>
              </td>
              <td class="currentgrade">
               <div><%= arenroll.artwothreecurrentgrade %></div>
              </td>
            </tr>

            <tr>
             <td class="current">
                <div><%= arenroll.artwofourcurrent %></div>
             </td>
             <td class="arorop">
               <div><%= arenroll.twofour %></div>
             </td>
             <td class="room">
              <div><%= arenroll.artwofourroom %></div>
             </td>
             <td class="purpose">
              <div><%= arenroll.artwofourpurpose %></div>
             </td>
             <td class="name">
               <div><%= arenroll.artwofourname %></div>
             </td>
             <td class="sort">
              <div><%= arenroll.artwofoursort %></div>
             </td>
             <td class="sex">
              <div><%= arenroll.artwofoursex %></div>
             </td>
             <td class="currentgrade">
              <div><%= arenroll.artwofourcurrentgrade %></div>
             </td>
           </tr>

           <tr>
            <td class="current">
               <div><%= arenroll.artwofivecurrent %></div>
            </td>
            <td class="arorop">
              <div><%= arenroll.twofive %></div>
            </td>
            <td class="room">
             <div><%= arenroll.artwofiveroom %></div>
            </td>
            <td class="purpose">
             <div><%= arenroll.artwofivepurpose %></div>
            </td>
            <td class="name">
              <div><%= arenroll.artwofivename %></div>
            </td>
            <td class="sort">
             <div><%= arenroll.artwofivesort %></div>
            </td>
            <td class="sex">
             <div><%= arenroll.artwofivesex %></div>
            </td>
            <td class="currentgrade">
             <div><%= arenroll.artwofivecurrentgrade %></div>
            </td>
          </tr>

          <tr>
           <td class="current">
              <div><%= arenroll.artwosixcurrent %></div>
           </td>
           <td class="arorop">
             <div><%= arenroll.twosix %></div>
           </td>
           <td class="room">
            <div><%= arenroll.artwosixroom %></div>
           </td>
           <td class="purpose">
            <div><%= arenroll.artwosixpurpose %></div>
           </td>
           <td class="name">
             <div><%= arenroll.artwosixname %></div>
           </td>
           <td class="sort">
            <div><%= arenroll.artwosixsort %></div>
           </td>
           <td class="sex">
            <div><%= arenroll.artwosixsex %></div>
           </td>
           <td class="currentgrade">
            <div><%= arenroll.artwosixcurrentgrade %></div>
           </td>
         </tr>
              <% }) %>

             </tbody>
            </table>

          </div> <br><br>

        </div>
        <!-- /.col-lg-12 -->
    </div>

</div>
<!-- /#page-wrapper -->

<% include ../parts/footer %>

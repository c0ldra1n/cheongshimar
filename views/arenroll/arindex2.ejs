<% include ../parts/head %>
<% include ../parts/othernav %>

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">AR 2층 신청현황</h1>
        </div>
        <!-- /.col-lg-12 선야 신청현황이 기본index임!!-->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">

          <h3>2-1 신청현황</h3>
          <div class="form-group pull-right">
            <input type="text" class="search form-control" value="2-1" disabled>
          </div>
          <span class="counter pull-right"></span>
          <table class="table table-hover table-bordered results">
           <thead>
            <tr>
              <th class="current">신청시간</th>
              <th class="arorop">분류</th>
              <th class="room">세미나실</th>
              <th class="purpose">이용목적</th>
              <th class="name">신청자</th>
              <th class="sort">계열</th>
              <th class="sex">성별</th>
              <th class="currentgrade">학년</th>
            </tr>
           </thead>
           <tbody>
            <% if(arenroll == null || arenroll.length == 0){ %>
             <tr>
              <td class="noData" colspan=100> 앗, 아무도 신청하지 않았네요! </td>
             </tr>
            <% } %>

            <% arenroll.forEach(function(arenroll) { %>
             <tr>
              <td class="current">
                 <div><%= arenroll.arcurrent %></div>
              </td>
              <td class="arorop">
                <div><%= arenroll.arar %></div>
              </td>
              <td class="room">
               <div><%= arenroll.arroom %></div>
              </td>
              <td class="purpose">
               <div><%= arenroll.arpurpose %></div>
              </td>
              <td class="name">
                <div><%= arenroll.arname %></div>
              </td>
              <td class="sort">
               <div><%= arenroll.arsort %></div>
              </td>
              <td class="sex">
               <div><%= arenroll.arsex %></div>
              </td>
              <td class="currentgrade">
               <div><%= arenroll.arcurrentgrade %></div>
              </td>
            <% }) %>
           </tbody>
         </table>

          <br><hr>

          <h3>2-2 신청현황</h3>
          <div class="form-group pull-right">
            <input type="text" class="search form-control" value="2-2" disabled>
          </div>
          <span class="counter pull-right"></span>
          <table class="table table-hover table-bordered results">
           <thead>
            <tr>
              <th class="current">신청시간</th>
              <th class="arorop">분류</th>
              <th class="room">세미나실</th>
              <th class="purpose">이용목적</th>
              <th class="name">신청자</th>
              <th class="sort">계열</th>
              <th class="sex">성별</th>
              <th class="currentgrade">학년</th>
            </tr>
           </thead>
           <tbody>
            <% if(arenroll == null || arenroll.length == 0){ %>
             <tr>
              <td class="noData" colspan=100> 앗, 아무도 신청하지 않았네요! </td>
             </tr>
            <% } %>

            <% arenroll.forEach(function(arenroll) { %>
             <tr>
              <td class="current">
                 <div><%= arenroll.arcurrent %></div>
              </td>
              <td class="arorop">
                <div><%= arenroll.arar %></div>
              </td>
              <td class="room">
               <div><%= arenroll.arroom %></div>
              </td>
              <td class="purpose">
               <div><%= arenroll.arpurpose %></div>
              </td>
              <td class="name">
                <div><%= arenroll.arname %></div>
              </td>
              <td class="sort">
               <div><%= arenroll.arsort %></div>
              </td>
              <td class="sex">
               <div><%= arenroll.arsex %></div>
              </td>
              <td class="currentgrade">
               <div><%= arenroll.arcurrentgrade %></div>
              </td>
            <% }) %>
           </tbody>
         </table>

          <br><br><br>
        </div>
        <!-- /.col-lg-12 -->
    </div>

</div>
<!-- /#page-wrapper -->

<% include ../parts/footer %>
